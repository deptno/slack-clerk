{
  "name": "slack-clerk",
  "version": "0.1.0",
  "description": "It's clerk for you team",
  "main": "handler.js",
  "scripts": {
    "setup": "serverless dynamodb install",
    "start:db": "serverless dynamodb start --migrate",
    "start:dev": "serverless offline",
    "test": "jest"
  },
  "devDependencies": {
    "@types/aws-lambda": "0.0.32",
    "@types/aws-sdk": "^2.7.0",
    "@types/jest": "^22.1.3",
    "@types/node": "^8.9.4",
    "@types/node-fetch": "^1.6.7",
    "aws-sdk": "^2.197.0",
    "jest": "^22.3.0",
    "serverless-dynamodb-local": "^0.2.27",
    "serverless-offline": "^3.18.0",
    "serverless-webpack": "^4.3.0",
    "ts-jest": "^22.0.4",
    "ts-loader": "^3.5.0",
    "typescript": "^2.7.2",
    "webpack": "^3.11.0"
  },
  "author": "Bonggyun Lee <deptno@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "get-urls": "^7.2.0",
    "node-fetch": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deptno/slack-clerk.git"
  },
  "keywords": [
    "slack",
    "slack-app",
    "slack-clerk",
    "clerk"
  ],
  "bugs": {
    "url": "https://github.com/deptno/slack-clerk/issues"
  },
  "homepage": "https://github.com/deptno/slack-clerk#readme",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/*.(test|spec).(ts|tsx)"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "enzyme.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ]
  }
}
